  <process id="applyV2" name="applyV2流程" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="${applyuserid}"></startEvent>
    <userTask id="managerApproveTask" name="经理审批" activiti:candidateGroups="部门经理"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="managerApproveTask"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="managerApproveTask" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="hrApproveTask" name="HR审批" activiti:candidateGroups="人事"></userTask>
    <sequenceFlow id="flow3" sourceRef="exclusivegateway1" targetRef="hrApproveTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerapprove=='true'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="reapplyTask" name="重新申请" activiti:assignee="${applyuserid}"></userTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="reapplyTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerapprove=='false'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow8" sourceRef="hrApproveTask" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="exclusivegateway3" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrapprove=='true'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="exclusivegateway3" targetRef="reapplyTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrapprove=='false'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow12" sourceRef="reapplyTask" targetRef="exclusivegateway4"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="exclusivegateway4" targetRef="managerApproveTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${goto=='manager'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway4" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${goto=='end'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="exclusivegateway4" targetRef="hrApproveTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${goto=='hr'}]]></conditionExpression>
    </sequenceFlow>
  </process>
